# .zshrc file for interactive zsh shells.                                                            
                                                                                                                                                                                                                
source ~/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh
eval "$(starship init zsh)"
eval "$(atuin init zsh)"  

# load direnv hook if not running as root                                                                        
if ! [ $(id -u) = 0 ]; then                                                                  
    eval "$(direnv hook zsh)"                                              
fi      